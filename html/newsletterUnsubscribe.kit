<body>
    <main>
        <section id="newsletter-delete">
            <div class="newsletter-delete center">
                <div class="newsletter-delete__graphics newsletter-delete__graphics--hand-glasses center"></div>
                <div class="wrapper">
                    <div class="newsletter-delete__box">
                        <div class="newsletter-delete__title">
                            <h2>Wypisz swojego maila z listy
                            </h2>
                        </div>
                        <div class="newsletter-delete__form-box">
                            <form registration-form="unsubscribe" class="newsletter-delete__form">
                                <div form-popup class="form-popup">
                                    <div form-popup-container class="form-popup__container">
                                        <button close-popup-btn class="form-popup__container--closeBtn"
                                            aria-label="close-popup-btn">Zamknij</button>
                                    </div>
                                </div>
                                <div class="newsletter-delete__form-row">
                                    <label for="userEmailUs">E-mail</label>
                                    <input type="text" name="userEmailUs" id="userEmailUs"
                                        placeholder="(np. imie@domena.pl)">
                                </div>
                                <div class="newsletter-delete__agree-box">
                                    <span class="input-checkbox"> <input name="checkboxUs" type="checkbox"
                                            id="unsubscribe"></span>
                                    <label for="unsubscribe">
                                        Jestem pewiene, że chcę usunąć swojego mail z listy </label>

                                </div>
                                <div class="newsletter-delete__send-box">

                                    <button class="newsletter-delete__submit" type="submit" aria-label="send-form-btn">
                                    </button>
                                    <p class="sendingProceed">Wysyłanie</p>
                                </div>
                            </form>


                        </div>


                    </div>
                </div>
            </div>
            </div>
        </section>

